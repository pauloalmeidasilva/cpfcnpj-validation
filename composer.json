{
    "name": "pauloalmeidasilva/cpfcnpj-validation",
    "description": "Biblioteca para validação e geração de CPF e CNPJ (pt-BR).",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "cpf",
        "cnpj",
        "validation",
        "br",
        "pt-br",
        "document",
        "validator"
    ],
    "autoload": {
        "psr-4": {
            "Src\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Paulo",
            "email": "paulocdjsilva@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0"
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "check-syntax": "php -l src/* || true"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
